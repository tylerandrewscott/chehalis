    "    \"cache_dir\": \"./textract_cache\",  # Cache directory\n",
    "    \"confidence_threshold\": 0.8,  # Minimum confidence for field extraction\n",
    "    \"use_forms_analysis\": True,  # Enable forms feature\n",
